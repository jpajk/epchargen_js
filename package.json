{
  "name": "lease-stats",
  "version": "1.0.0",
  "description": "For keeping track of my lease stats",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "clean": "rm -rf lib",
    "dev-server": "node lib/server/webpack",
    "watch-js": "babel src -d lib -w",
    "watch-all": "gulp stream-all",
    "start": "npm run watch-js & npm run watch-all & npm run dev-server & npm run server & npm run webpack",
    "start-debug": "npm run watch-js & npm run watch-all & npm run dev-server & npm run server-debug & npm run webpack",
    "babel": "babel src -d lib",
    "copy-all": "gulp copy-all",
    "webpack": "webpack -w --config src/conf/webpack.config.production.js",
    "server": "nodemon lib/server/server",
    "server-debug": "nodemon --debug lib/server/server",
    "symlink": "node -e \"var s='../lib',d='node_modules/app',fs=require('fs');fs.exists(d,function(e){e||fs.symlinkSync(s,d,'dir')});\"",
    "postinstall": "npm run babel && npm run copy-all && npm run symlink && npm run webpack && gulp remove-less-from-lib",
    "load-fixtures": "node ./lib/fixtures/aptitudes.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akhaku/lease-stats.git"
  },
  "author": "shikohin",
  "license": "Unlicense",
  "engines": {
    "node": "4.2.3",
    "npm": "2.14.7"
  },
  "private": true,
  "dependencies": {
    "async": "^2.1.5",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-class-properties": "^6.5.2",
    "babel-plugin-transform-decorators": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "bcrypt": "^1.0.2",
    "connect-multiparty": "^2.0.0",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.26.1",
    "del": "^2.2.0",
    "express": "^4.13.4",
    "express-session": "^1.15.1",
    "gulp": "^3.9.1",
    "gulp-print": "^2.0.1",
    "gulp-watch": "^4.3.5",
    "history": "^4.5.1",
    "lodash": "^4.3.0",
    "merge-stream": "^1.0.0",
    "mobx": "^3.1.0",
    "mobx-react": "^4.1.0",
    "mysql": "^2.13.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.8.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pr-express-body-parser": "^0.1.4",
    "pug": "^2.0.0-beta11",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "sass": "^0.5.0",
    "sass-loader": "^6.0.2",
    "sequelize": "^3.30.2",
    "sequelize-fixtures": "^0.5.6",
    "style-loader": "^0.13.0",
    "webpack": "2.2.1",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "eslint": "^3.15.0",
    "eslint-plugin-react": "^6.9.0",
    "react-hot-loader": "^1.3.0",
    "webpack-dev-server": "^2.3.0"
  }
}
